user www-data;
worker_processes auto;
error_log /var/log/nginx/error.log;
pid /run/nginx.pid;

events {
    worker_connections 1024;
}

http {
    server {
        listen [2a01:4f9:4a:27c1::253]:80;
        server_name craftbike.toadres.pl;

        location / {
            proxy_pass http://localhost:8003;
        }
    }
}